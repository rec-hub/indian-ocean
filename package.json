{
  "name": "indian-ocean",
  "version": "2.0.1",
  "description": "A library for reading in and writing out data in Node.js.",
  "main": "lib/index.js",
  "scripts": {
    "coverage": "istanbul cover mocha -- -R spec",
    "docs": "documentation -g -f html -o docs -t ./docs/theme/",
    "serve-docs": "http-server docs -p 8080 -o",
    "build-view": "npm run docs && npm run serve-docs",
    "test": "standard && mocha",
    "standard-fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mhkeller/indian-ocean"
  },
  "keywords": [
    "data",
    "read",
    "write",
    "csv",
    "json",
    "psv",
    "tsv",
    "dbf",
    "fs",
    "fs-plus",
    "convert",
    "files",
    "io"
  ],
  "author": "Michael Keller (http://mhkeller.com)",
  "contributors": [
    "Eric Brelsford (https://ebrelsford.github.io/portfolio/)",
    "Julian Burgess (https://aubergene.com)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mhkeller/indian-ocean/issues"
  },
  "homepage": "https://github.com/mhkeller/indian-ocean",
  "readmeFilename": "README.md",
  "dependencies": {
    "archieml": "^0.4.2",
    "chalk": "^1.1.3",
    "d3-dsv": "git+https://github.com/rec-hub/d3-dsv.git",
    "dbf": "^0.1.2",
    "js-yaml": "^3.4.2",
    "mkdirp": "^0.5.1",
    "parse-json": "^2.2.0",
    "d3-queue": "^3.0.3",
    "shapefile": "^0.3.0",
    "underscore": "^1.8.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "documentation": "2.1.0-alpha1",
    "http-server": "^0.9.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.0",
    "rimraf": "^2.5.4",
    "standard": "^8.3.0"
  }
}
